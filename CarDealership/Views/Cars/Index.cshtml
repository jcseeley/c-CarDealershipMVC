<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>My To-Do List!</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
  </head>
  <body>
    <div class="container">
      @using CarDealership.Models;

      <h1>List of Cars</h1>

      @if (Model.Count == 0)
      {
        <p>There are no cars on your list.</p>
      }
      <ul>
        @foreach (Car automobile in Model)
        {
          <li>@automobile.MakeModel, Price: $@automobile.Price, Miles: @automobile.Miles</li>
        }
      </ul>
      <a href="/cars/new">Add Another Car</a><br>
      <a href="/discount">Discount Cars</a>
  </body>
</html>